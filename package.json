{
  "name": "@dlwiest/ts-tonal-client",
  "version": "0.1.1",
  "description": "TypeScript client for Tonal API",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "typecheck": "tsc --noEmit",
    "dev": "tsc --watch --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run typecheck && npm run test && npm run build",
    "example:movements": "tsx examples/get-movements.ts",
    "example:movements:save": "tsx examples/get-movements.ts --save",
    "example:workout:id": "tsx examples/get-workout-by-id.ts",
    "example:workout:share": "tsx examples/get-workout-by-share-url.ts",
    "example:user": "tsx examples/get-user-info.ts",
    "example:goals": "tsx examples/get-goals.ts",
    "example:user-workouts": "tsx examples/get-user-workouts.ts",
    "example:estimate": "tsx examples/estimate-workout.ts",
    "example:create-workout": "tsx examples/create-workout.ts",
    "example:edit-workout": "tsx examples/edit-workout.ts",
    "example:delete-workout": "tsx examples/delete-workout.ts",
    "example:daily-lifts": "tsx examples/get-daily-lifts.ts",
    "example:training-effect-goals": "tsx examples/get-training-effect-goals.ts",
    "example:training-types": "tsx examples/get-training-types.ts",
    "example:goal-metrics": "tsx examples/get-goal-metrics.ts",
    "example:user-settings": "tsx examples/get-user-settings.ts",
    "example:daily-metrics": "tsx examples/get-daily-metrics.ts",
    "example:current-streak": "tsx examples/get-current-streak.ts",
    "example:activity-summaries": "tsx examples/get-activity-summaries.ts",
    "example:user-statistics": "tsx examples/get-user-statistics.ts",
    "example:achievement-stats": "tsx examples/get-achievement-stats.ts",
    "example:achievements": "tsx examples/get-achievements.ts",
    "example:home-calendar": "tsx examples/get-home-calendar.ts",
    "example:muscle-readiness": "tsx examples/get-muscle-readiness.ts",
    "example:program-by-id": "tsx examples/get-program-by-id.ts",
    "example:target-scores": "tsx examples/get-target-scores.ts",
    "example:metric-scores": "tsx examples/get-metric-scores.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dlwiest/ts-tonal-client.git"
  },
  "keywords": [
    "tonal",
    "tonal-api",
    "typescript",
    "client"
  ],
  "author": "dlwiest",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dlwiest/ts-tonal-client/issues"
  },
  "homepage": "https://github.com/dlwiest/ts-tonal-client#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.14.8",
    "dotenv": "^16.4.5",
    "jest": "^30.2.0",
    "rollup": "^3.20.2",
    "rollup-plugin-dts": "^5.3.1",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "tsx": "^4.20.6",
    "typescript": "^5.5.4"
  }
}
